<!doctype html>
<html lang="en" ng-app="air-menu">
	<head>
		<meta charset="utf-8">
        <%= csrf_meta_tag %>
		<title>AirMenu</title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<link href='http://fonts.googleapis.com/css?family=Roboto:400,300italic,300,100italic,100,400italic,500,500italic,700,700italic' rel='stylesheet' type='text/css'>
        <link rel="stylesheet" type="text/css" href="<%= asset_path('application.css') %>"/>
        <script src="<%= asset_path('application.js') %>"></script>
      <script>
        window.CSRF_TOKEN = '<%= form_authenticity_token %>';
      </script>
	</head>
	<body ng-controller="MainCtrl">
		<navbar user="user"></navbar>
		<ng-view></ng-view>
	</body>
</html>
